<plugin>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://examplemodel/1.0"
            class="de.hub.xveg.example.examplemodel.ExampleModelPackage"
            genModel="src/de/hub/xveg/example/examplemodel/examplemodel.genmodel"/>
   </extension>
   
	   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            id="de.hub.xveg.example.ExampleDiagram"
            name="Xveg Example Diagram"
            type="xed">
      </diagramType>
   </extension>
   
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="de.hub.xveg.example.ExampleDiagramTypeProvider"
            id="de.hub.xveg.example.ExampleDiagramProvider"
            name="Xveg Example Diagram">
         <diagramType
               id="de.hub.xveg.example.ExampleDiagram">
         </diagramType>
         <imageProvider
               id="de.hub.xveg.ImageProvider">
         </imageProvider>
         <imageProvider
               id="org.eclipse.graphiti.examples.common.ExampleImageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="edu.hu.clickwatch.analysis.WizardCategory"
            class="de.hub.xveg.example.NewDiagramWizard"
            icon="icons/sample.gif"
            id="de.hub.xveg.file.NewWizard"
            name="New Diagram">
      </wizard>
   </extension>
</plugin>
